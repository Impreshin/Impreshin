{% include "_nav_top.tmpl" %}
<section id="maintoolbar">
	<div class="container">
		<div class="toolbar clearfix  ">
			<ul class="nav nav-pills pull-left">


				<li class="divider-vertical"></li>
			</ul>
			<div class="pull-right">
				<ul class="nav nav-pills pull-left" id="provisional-stats-bar">

				</ul>
				<ul class="nav nav-pills pull-right ">
					<li class="divider-vertical" style="margin-right: 0;"></li>
					<li class="dropdown pull-right">
						<a id="reload-btn" href="javascript:" style="width:15px;"><i class="icon-refresh"></i></a>

					</li>
				</ul>
			</div>

		</div>
		<div class="toolbar-end"></div>
	</div>

</section>

<section role="main" id="pagecontent">
	<div class="container">


			<article id="whole-area" class="span16">
				<div class="content">
					<div class="scroll-pane scrolly" id="dummy-area">

						<div data-heading="top" class="scroll-container" style="padding-left: 15px;" id="pages-area">



						</div>

					</div>

				</div>
				<div id="dummy-bottom" class="highlighthovered">

				</div>
				<div class="loadingmask wide"></div>

			</article>

		</div>




</section>

<script type="text/javascript">
	var pageSize = "0";
	var cmSize = "0";
	var colSize = "0";
	var colcmAv = "0";
	{% if _ab['publication']['pagewidth'] and _ab['publication']['cmav'] %}
		pageSize ="{{ (((143/( _ab['publication']['pagewidth'] / 100 ))*_ab['publication']['cmav'])/10) }}";
		cmSize ="{{ ((((143/( _ab['publication']['pagewidth'] / 100 ))*_ab['publication']['cmav'])/10)) / _ab['publication']['cmav'] }}";
		colSize ="{{ 143 / _ab['publication']['columnsav'] }}";
		colcmAv = "{{  _ab['publication']['cmav'] * _ab['publication']['columnsav'] }}";
	{% endif %}

</script>
